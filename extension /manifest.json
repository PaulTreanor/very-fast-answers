{
    "manifest_version": 3,
    "name": "ZapSearch ⚡️",
    "description": "Access LLMs from your search bar",
    "version": "0.1",
    "action": {
      "default_icon": "border-48.png"
    },
    "permissions": ["tabs", "scripting"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
        "service_worker": "background.js"
    }
  }